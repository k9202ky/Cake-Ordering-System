{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/stylesheets/(.*)",
      "dest": "/public/stylesheets/$1"
    },
    {
      "src": "/(.*)\\.png",
      "dest": "/$1.png"
    },
    {
      "src": "/(.*)\\.ico",
      "dest": "/$1.ico"
    },
    {
      "src": "/site.webmanifest",
      "dest": "/site.webmanifest"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
